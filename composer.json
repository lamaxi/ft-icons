{
    "name": "lamaxi/ft-icons",
    "description": "A package to easily make use of \"EOS Icons\" in your Laravel Blade views.",
    "keywords": ["Blade", "eos-icons", "Laravel"],
    "homepage": "https://github.com/lamaxi/ft-icons",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Maksymchuk Andrii",
            "homepage": "https://laravel.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "codeat3/blade-icon-generation-helpers": "^0.8",
        "codeat3/phpcs-styles": "^1.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "BladeFreetrack\\BladeFreetrackIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "BladeFreetrack\\BladeFreetrackIcons\\BladeFreetrackIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix"
    }
}
